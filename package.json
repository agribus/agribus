{
  "name": "agribus",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\" && turbo run format",
    "check-types": "turbo run check-types",
    "clean": "turbo run clean",
    "ly:add": "pnpm -F lychee add",
    "to:add": "dotnet add ./apps/tomato_backend package"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.5.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
